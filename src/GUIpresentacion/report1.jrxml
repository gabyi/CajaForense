	<queryString language="SQL">
		<![CDATA[SELECT
     abogado.`codprof` AS Codigo,
     abogado.`nombre` AS Nombre,
     abogado.`apellido` AS Apellido,
     boletas.`fecha` AS Fecha,
     boletas.`monto_aportes` AS Aportes,
     abogado_has_boletas.`Boletas_idBoletas` AS Boleta,
     valorescajarentas.`materia` AS Juicio
FROM
     `abogado` abogado INNER JOIN `abogado_has_boletas` abogado_has_boletas ON abogado.`cuil` = abogado_has_boletas.`abogado_cuil`
     INNER JOIN `boletas` boletas ON abogado_has_boletas.`Boletas_idBoletas` = boletas.`idBoletas`
     INNER JOIN `valorescajarentas` valorescajarentas ON boletas.`ValoresCajaRentas_id` = valorescajarentas.`id`]]>
	</queryString>
